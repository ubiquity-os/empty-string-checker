{
  "$schema": "https://raw.githubusercontent.com/streetsidesoftware/cspell/main/cspell.schema.json",
  "version": "0.2",
  "ignorePaths": ["**/*.json", "**/*.css", "node_modules", "**/*.log", "coverage/**", ".git/**", ".idea/**", "bun.lock", "bun.xml", "build/**", "static/**"],
  "useGitignore": true,
  "language": "en",
  "words": [
    "dataurl",
    "devpool",
    "outdir",
    "servedir",
    "ubiquity",
    "ubiquity-os",
    "eslint",
    "esbuild",
    "prettier",
    "knip",
    "RuleTester",
    "tsconfig",
    "sonarjs",
    "workflows",
    "release-please",
    "tsnode",
    "prettierrc",
    "ZWNBSP",
    "FEFF",
    "ZWSP",
    "ZWNJ",
    "quasis",
    "TSES",
    "claration"
  ],
  "dictionaries": ["typescript", "node", "software-terms"],
  "import": ["@cspell/dict-typescript/cspell-ext.json", "@cspell/dict-node/cspell-ext.json", "@cspell/dict-software-terms"],
  "ignoreRegExpList": ["[0-9a-fA-F]{6}"]
}
